{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "neuro-exercises-app",
  "mainBinaryName": "Neuro Exercises",
  "version": "0.1.0",
  "identifier": "com.joaocupido.neuro-exercises",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Neuro Exercises",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
